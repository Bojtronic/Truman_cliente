@model List<Usuario>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    
</div>


<div class="card">
  <div class="card-header">
    @ViewBag.Accion
  </div>
  <div class="card-body">

        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Contrasena</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Nombre</td>
                        <td>@item.Contrasena</td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>


